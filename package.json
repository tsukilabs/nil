{
  "name": "nil",
  "version": "0.2.20",
  "type": "module",
  "packageManager": "pnpm@10.29.1",
  "license": "AGPL-3.0-only",
  "private": true,
  "scripts": {
    "build": "cargo +nightly -Zscript scripts/build-client.rs",
    "clippy": "cargo +nightly -Zscript scripts/run-clippy.rs",
    "dev": "cargo +nightly -Zscript scripts/start-dev.rs",
    "docs": "cargo +nightly doc -p nil-* --open --no-deps --lib --document-private-items",
    "format": "dprint fmt && cargo fmt --all",
    "lint": "eslint . --config eslint.config.ts",
    "lint-fix": "eslint . --config eslint.config.ts --fix",
    "miri": "cargo +nightly -Zscript scripts/test.rs --miri",
    "preview": "cargo +nightly -Zscript scripts/build-client.rs --preview",
    "server": "cargo +nightly -Zscript scripts/start-server.rs",
    "test": "cargo +nightly -Zscript scripts/test.rs",
    "type-check": "pnpm run -r --bail type-check"
  },
  "dependencies": {
    "@tauri-apps/api": "=2.10.1",
    "@tauri-apps/plugin-clipboard-manager": "=2.3.2",
    "@tauri-apps/plugin-dialog": "=2.6.0",
    "@tauri-apps/plugin-fs": "=2.4.5",
    "@tauri-apps/plugin-opener": "=2.5.3",
    "@tauri-apps/plugin-os": "=2.3.2",
    "@tauri-apps/plugin-process": "=2.3.1",
    "@tauri-apps/plugin-updater": "=2.10.0",
    "@tauri-store/pinia": "=4.1.1",
    "@tb-dev/prototype-dom": "=7.0.32",
    "@tb-dev/utils": "=7.2.2",
    "@tb-dev/vue": "=3.4.5",
    "@tb-dev/vue-components": "=7.0.8",
    "@vueuse/components": "=14.2.0",
    "@vueuse/core": "=14.2.0",
    "@vueuse/math": "=14.2.0",
    "@vueuse/router": "=14.2.0",
    "date-fns": "=4.1.0",
    "es-toolkit": "=1.44.0",
    "lucide-vue-next": "=0.563.0",
    "pinia": "=3.0.4",
    "reka-ui": "=2.8.0",
    "tw-animate-css": "=1.4.0",
    "vue": "=3.5.27",
    "vue-i18n": "=11.2.8",
    "vue-router": "=5.0.2",
    "vue-sonner": "=2.0.9",
    "zod": "=4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/vite": "=4.1.18",
    "@tauri-apps/cli": "=2.10.0",
    "@tb-dev/eslint-config": "=8.2.6",
    "@types/node": "=25.2.2",
    "@vitejs/plugin-vue": "=6.0.4",
    "eslint": "=9.39.2",
    "tailwindcss": "=4.1.18",
    "tslib": "=2.8.1",
    "typescript": "=5.9.3",
    "vite": "=7.3.1",
    "vitepress": "=2.0.0-alpha.16",
    "vue-tsc": "=3.2.4"
  }
