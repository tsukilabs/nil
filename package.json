{
  "name": "nil",
  "version": "0.1.23",
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "license": "AGPL-3.0-only",
  "private": true,
  "scripts": {
    "build": "pwsh scripts/build-bin.ps1",
    "clippy": "pwsh scripts/invoke-clippy.ps1",
    "dev": "pwsh scripts/start-dev.ps1",
    "docs": "cargo doc -p nil-* --open --no-deps --lib --all-features",
    "eslint": "eslint . --config eslint.config.js",
    "format": "dprint fmt && cargo fmt --all",
    "miri": "pwsh scripts/test.ps1 -Miri",
    "plugin": "pnpm run -F tauri-plugin-* build",
    "preview": "pwsh scripts/build-bin.ps1 -Preview",
    "test": "pwsh scripts/test.ps1",
    "type-check": "pnpm run -r --bail type-check",
    "wasm": "pwsh scripts/build-wasm.ps1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@tb-dev/eslint-config": "^8.0.5",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^9.33.0",
    "tailwindcss": "^4.1.12",
    "tslib": "^2.8.1",
    "typescript": "~5.9.2",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-wasm": "~3.5.0",
    "vue-tsc": "^3.0.5"
  }
}
