{
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "conventionalCommits.promptScopes": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.colorDecoratorsLimit": 5000,
  "editor.defaultFormatter": "dprint.dprint",
  "editor.formatOnSave": true,
  "eslint.codeActionsOnSave.mode": "problems",
  "eslint.useFlatConfig": true,
  "evenBetterToml.formatter.allowedBlankLines": 1,
  "evenBetterToml.formatter.columnWidth": 100,
  "evenBetterToml.formatter.reorderArrays": true,
  "evenBetterToml.formatter.trailingNewline": true,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "files.eol": "\n",
  "githubPullRequests.defaultMergeMethod": "squash",
  "rust-analyzer.cargo.targetDir": true,
  "rust-analyzer.linkedProjects": [
    "./Cargo.toml",
    "./scripts/build-client.rs",
    "./scripts/build-docs.rs",
    "./scripts/build-server.rs",
    "./scripts/run-clippy.rs",
    "./scripts/sort-namegen.rs",
    "./scripts/start-dev.rs",
    "./scripts/start-server.rs",
    "./scripts/test.rs"
  ],
  "workbench.editor.customLabels.patterns": {
    "**/App.vue": "${dirname}/App.vue",
    "**/build.rs": "${dirname}/build.rs",
    "**/Cargo.toml": "${dirname}/Cargo.toml",
    "**/index.ts": "${dirname}/index.ts",
    "**/index.vue": "${dirname}/index.vue",
    "**/mod.rs": "${dirname}/mod.rs",
    "**/package.json": "${dirname}/package.json",
    "**/tsconfig.json": "${dirname}/tsconfig.json",
    "*/*/**/lib.rs": "${dirname(1)}/lib.rs",
    "*/*/**/main.rs": "${dirname(1)}/main.rs",
    "*/Cargo.toml": "<workspace>/Cargo.toml",
    "*/package.json": "<workspace>/package.json",
    "*/tsconfig.json": "<workspace>/tsconfig.json"
  }
}
